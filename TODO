// should be before the line count = ft_lstsize(list);
void	check_value_export(t_list *list)
{
	int		i;
	int		j;
	char	**s;

	i = 0;
	j = 0;
	while (list->option[i])
	{
		if (ft_strlen(list->option[i]) > 0)
			j++;
		i++;
	}
	s = malloc(sizeof(char *) * (j + 1));
	if (!s)
		exit(1);
	i = 0;
	j = 0;
	while (list->option[i])
	{
		if (ft_strlen(list->option[i]) > 0)
		{
			s[j] = list->option[i];
			j++;
		}
		i++;
	}
	s[j] = NULL;
	list->option = s;
}
